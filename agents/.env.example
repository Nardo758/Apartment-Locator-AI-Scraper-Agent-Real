# Agent System Configuration
# Environment variables for the apartment scraper agents

# ===================================
# API KEYS
# ===================================

# SERP API for property URL discovery
SERP_API_KEY=your_serp_api_key_here

# Anthropic Claude API for property info extraction
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI API for vision model (Rental Data Agent)
OPENAI_API_KEY=your_openai_api_key_here

# ===================================
# SUPABASE CONFIGURATION
# ===================================

# Supabase project URL
SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase service role key (for server-side operations)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Supabase anon key (for client-side operations)
SUPABASE_ANON_KEY=your_anon_key_here

# ===================================
# AGENT CONFIGURATION
# ===================================

# Property Discovery Agent settings
DISCOVERY_MAX_PROPERTIES=200
DISCOVERY_LOCATION=Atlanta, GA
DISCOVERY_SERP_QUERIES=apartments for rent,apartment communities,luxury apartments,affordable apartments

# Claude model settings
CLAUDE_MODEL=claude-3-haiku-20240307
CLAUDE_MAX_TOKENS=1000
CLAUDE_TEMPERATURE=0.1

# Rental Data Agent settings
VISION_MODEL=gpt-4-vision-preview
VISION_MAX_TOKENS=2000
BROWSER_TIMEOUT=30000
PAGE_LOAD_TIMEOUT=10000

# ===================================
# COST CONTROL
# ===================================

# Maximum cost per discovery batch (USD)
MAX_DISCOVERY_COST=5.0

# Maximum cost per rental extraction (USD)
MAX_RENTAL_COST=10.0

# Priority thresholds
HIGH_PRIORITY_UNIT_THRESHOLD=200
LUXURY_KEYWORDS=luxury,premier,executive,high-end
MAJOR_MANAGEMENT_COMPANIES=CBRE,Greystar,Equity Residential,AvalonBay,Berkshire Hathaway,Thrive Communities

# ===================================
# LOGGING & MONITORING
# ===================================

LOG_LEVEL=INFO
LOG_FILE=agent_system.log
ENABLE_COST_TRACKING=true
ENABLE_PERFORMANCE_MONITORING=true